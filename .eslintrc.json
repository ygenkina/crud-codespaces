{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-undef": 2,
    "no-unused-vars": 0,
    "no-console": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "no-empty": 0,
    "react/no-array-index-key": 0
  }
}